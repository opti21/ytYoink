{
  "name": "Youtube Title Yoinker",
  "version": "1.0",
  "description": "Send the title of currently playing youtube video to Twitch chat",
  "permissions": ["tabs", "declarativeContent"],
  "background": {
    "scripts": ["comfy.min.js", "background.js"],
    "persistent": true
  },
  "icons": {
    "16": "images/opti_16.png",
    "32": "images/opti_32.png",
    "48": "images/opti_48.png",
    "128": "images/opti_128.png"
  },
  "manifest_version": 2
}
